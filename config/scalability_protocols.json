{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout_ms": 5000,
		"debug_endpoints": false
	},
	"input": {
		"scalability_protocols": {
			"bind": true,
			"poll_timeout_ms": 5000,
			"reply_timeout_ms": 5000,
			"socket_type": "PULL",
			"sub_filters": [],
			"urls": [
				"tcp://*:5555"
			]
		},
		"type": "scalability_protocols"
	},
	"buffer": {
		"none": {},
		"type": "none"
	},
	"pipeline": {
		"processors": [
			{
				"bounds_check": {
					"max_part_size": 1073741824,
					"max_parts": 100,
					"min_part_size": 1,
					"min_parts": 1
				},
				"type": "bounds_check"
			}
		],
		"threads": 1
	},
	"output": {
		"scalability_protocols": {
			"bind": false,
			"poll_timeout_ms": 5000,
			"socket_type": "PUSH",
			"urls": [
				"tcp://localhost:5556"
			]
		},
		"type": "scalability_protocols"
	}
}