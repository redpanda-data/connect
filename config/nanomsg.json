{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout_ms": 5000,
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "nanomsg",
		"nanomsg": {
			"bind": true,
			"poll_timeout_ms": 5000,
			"reply_timeout_ms": 5000,
			"socket_type": "PULL",
			"sub_filters": [],
			"urls": [
				"tcp://*:5555"
			]
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "nanomsg",
		"nanomsg": {
			"bind": false,
			"poll_timeout_ms": 5000,
			"socket_type": "PUSH",
			"urls": [
				"tcp://localhost:5556"
			]
		}
	},
	"resources": {
		"caches": {},
		"conditions": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true
	},
	"metrics": {
		"type": "http_server",
		"prefix": "benthos",
		"http_server": {},
		"prometheus": {},
		"statsd": {
			"address": "localhost:4040",
			"flush_period": "100ms",
			"network": "udp"
		}
	}
}